
@model FUTOMedical.Models.Entities.Setting
@using FUTOMedical.Helpers
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_FutoLayout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header titlehms"><i class="fa fa-info-circle"></i>Info<i class="fa fa-info-circle"></i></h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="panel panel-info">
    <div class="panel-heading">
        <i class="fa fa-info-circle fa-fw"></i>Setting Information
    </div>
    <!-- /.panel-heading -->
    <div class="col-lg-2"></div>
    <div class="row">

        <div class="col-lg-8">
            <div class="panel-body">



                <div class="form-group ">
                    <label>Application Title:</label>
                    <span> @Model.ApplicationTitle</span>

                </div>
                <div class="form-group ">
                    <label>Initial:</label>
                    <span> @Model.Initial</span>

                </div>
                <div class="form-group ">
                    <label>Email Address:</label>
                    <span>@Model.EmailAddress</span>

                </div>

                <div class="form-group ">
                    <label>Phone:</label>
                    <span>@Model.PhoneNo</span>

                </div>
                <div class="form-group ">
                    <label>Currency:</label>
                    <span>@Model.Currency</span>

                </div>

                <div class="form-group ">
                    <label>Favicon:</label>
                    <img src="@Url.Content(Model.Favicon)" alt="" width="200" height="100" class=" img-thumbnail">
                    @*<span>@Url.Content(Model.Favicon)</span>*@

                </div>

                <div class="form-group ">
                    <label>Logo:</label>
                    <img src="@Url.Content(Model.Logo)" alt="" width="200" height="100" class=" img-thumbnail">
                    @*<span>@Url.Content(Model.Logo)</span>*@

                </div>
                <p class="text-center">
                    <div class="btn-group">

                        <a class="btn btn-warning" href="@Url.Action("Edit", "Settings", new { id = Model.Id, area = "Admin" })"> <i class='fa fa-plus-circle fa-fw'></i>Edit</a>


                        <a class="btn btn-default" href="javascript:history.back()">Back</a>
                    </div>

            </div>
        </div>
        <div class="col-lg-2"></div>


    </div>
</div>



















