@model FUTOMedical.Models.Entities.Department

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_FutoLayout.cshtml";
}


<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header titlehms"><i class="fa fa-info-circle"></i>Info<i class="fa fa-info-circle"></i></h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<div class="panel panel-info">
    <div class="panel-heading">
        <i class="fa fa-info-circle fa-fw"></i>Department Information
    </div>
    <!-- /.panel-heading -->
    <div class="col-lg-2"></div>
    <div class="row">

        <div class="col-lg-8">
            <div class="panel-body">



                <div class="form-group ">
                    <label>@Html.DisplayNameFor(model => model.Name):</label>
                    <span> @Html.DisplayFor(model => model.Name)</span>

                </div>

                <div class="form-group ">
                    <label>@Html.DisplayNameFor(model => model.Description):</label>
                    <span>@Html.DisplayFor(model => model.Description)</span>

                </div>

            </div>

        </div>
        <div class="col-lg-2"></div>


    </div>
</div>

